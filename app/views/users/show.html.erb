
<div class="span12">
	
<h2>User</h2>	<br/>
<table class="table table-bordered table-striped">
	<thead>
		<tr>	
		<th>Name</th>
		<th>Email</th>
		</tr>	
	</thead>
	<tbody>	
		<% @user.each do |u| %>
			<tr>
				<td>
					<%= u.name %>
				</td>
				<td>
					<%= u.email %>
				</td>
			</tr>
		<%end %>
	</tbody>
	</table>
	
</div>